<table border="1">
    <thead>
      <tr>
        <th>ID</th>
        <th>Title</th>
        <th>Due Date</th>
        <th>Completed</th>
      </tr>
    </thead>
    <tbody>
      <% allTodos.forEach(todo => { %>
        <tr>
          <td><%= todo.id %></td>
          <td><%= todo.title %></td>
          <td><%= todo.dueDate %></td>
          <td><%= todo.completed %></td>
        </tr>
      <% }) %>
    </tbody>
  </table>
  